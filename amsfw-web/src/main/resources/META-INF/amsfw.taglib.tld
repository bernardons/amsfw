<?xml version="1.0"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>u</short-name>
	<uri>http://com.unisys.br/awsfw</uri>
	<display-name>AMS Framework - Components Library</display-name>
	<description> This tag library contains ui compositions of AMS
		Framework from Unisys.
	</description>
	<!-- >>>>>>>>>>>>>>>>> ARQUIVO <<<<<<<<<<<<<<<<< -->
	<tag>
		<name>arquivo</name>
		<tag-class />
		<body-content>empty</body-content>
		<description>Cria um item de formulário com label, campo para carregar
			arquivo(s) e Mensagem</description>
		<attribute>
			<name>identificador</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Identificador do item
			</description>
		</attribute>
		<attribute>
			<name>label</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Texto exibido no label do componente
			</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Valor associado ao inputText do componente
			</description>
		</attribute>
		<attribute>
			<name>required</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.Boolean</type>
			<description> Determinar se o campo é de preenchimento obrigatório.
			</description>
		</attribute>
		<attribute>
			<name>requiredMessage</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Mensagem que será exibida quando o campo for
				obrigatório e não tiver sido preenchido.
			</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.Boolean</type>
			<description> Determina se o campo é habilitado.
			</description>
		</attribute>
	</tag>
	<!-- >>>>>>>>>>>>>>>>> AUTO COMPLETE <<<<<<<<<<<<<<<<< -->
	<tag>
		<name>autoComplete</name>
		<tag-class />
		<body-content>empty</body-content>
		<description>Cria uma caixa de texto com a função de autocomplete.</description>
		<attribute>
			<name>identificador</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Define o identificador do componente.
			</description>
		</attribute>
		<attribute>
			<name>label</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Texto exibido no label do componente
			</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Valor associado ao inputText do componente
			</description>
		</attribute>
		<attribute>
			<name>bean</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Controller que será referenciado. Ex:
				"#{usuarioPersistController}".
			</description>
		</attribute>
		<attribute>
			<name>required</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.Boolean</type>
			<description> Determinar se o campo é de preenchimento obrigatório.
			</description>
		</attribute>
		<attribute>
			<name>requiredMessage</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Mensagem que será exibida quando o campo for
				obrigatório e não tiver sido preenchido.
			</description>
		</attribute>
		<attribute>
			<name>update</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Comprimento da caixa de texto. Somente valores de 1 a
				12.
			</description>
		</attribute>
		<attribute>
			<name>size</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Comprimento da caixa de texto. Somente valores de 1 a
				12.
			</description>
		</attribute>
		<attribute>
			<name>method</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Método que executará a ação de buscar as palavras para
				o autocomplete
			</description>
		</attribute>
		<attribute>
			<name>itemLabel</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Texto de cada valor retornado daquilo que foi digitado.
			</description>
		</attribute>
		<attribute>
			<name>ajaxAutoc</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.Boolean</type>
			<description> Habilita o Ajax para o autocomplete
			</description>
		</attribute>
		<attribute>
			<name>beanAjax</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Controller que será referenciado para o autocomplete.
				Ex: "#{usuarioPersistController}".
			</description>
		</attribute>
		<attribute>
			<name>methodAjax</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Método que irá executar uma ação quando o usuário
				escolher uma palavra no autocomplete.
			</description>
		</attribute>
		<attribute>
			<name>updateAutoc</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Componente que será renderizado quando o usuário
				escolher uma palavra no autocomplete.
			</description>
		</attribute>
		<attribute>
			<name>minQueryLength</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Mínimo de caracteres para iniciar a pesquisa de
				palavras para o autocomplete.
			</description>
		</attribute>
		<attribute>
			<name>maxLength</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description> Maximo de caracteres para o campo. Default 8.
			</description>
		</attribute>
		<attribute>
			<name>readOnly</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.Boolean</type>
			<description> Indica que o campo será somente para leitura.
			</description>
		</attribute>
		<attribute>
			<name>sizeInput</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description />
		</attribute>
		<attribute>
			<name>maxResults</name>
			<rtexprvalue>false</rtexprvalue>
			<type>java.lang.String</type>
			<description>Indica o numero maximo de resultados . Default: 15</description>
		</attribute>
	</tag>
